{
  "name": "tr-web",
  "version": "0.0.0",
  "scripts": {
    "build:ci": "pnpm install && pnpm build",
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "start": "tinacms build && next start",
    "lint": "next lint",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks --project-name=saas-starter",
    "generate-types": "npx supabase gen types typescript --project-id YOUR_PROJECT_ID --schema public > types_db.ts",
    "prepare": "husky install",
    "pre-commit": "pnpm format && pnpm lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "clean": "rm -rf node_modules && pnpm install",
    "test:e2e": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@hookform/resolvers": "^3.2.0",
    "@next/eslint-plugin-next": "^13.4.19",
    "@stripe/stripe-js": "^1.53.0",
    "@supabase/auth-helpers-nextjs": "^0.7.0",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/auth-ui-shared": "^0.1.6",
    "@supabase/supabase-js": "^2.23.0",
    "@tinacms/cli": "^1.5.31",
    "antd": "^5.8.3",
    "axios": "^1.5.1",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "highcharts": "^11.1.0",
    "highcharts-react-official": "^3.2.0",
    "next": "13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-merge-refs": "^2.0.2",
    "react-number-format": "^5.3.0",
    "react-slick": "^0.29.0",
    "sharp": "^0.32.6",
    "slick-carousel": "^1.8.1",
    "stripe": "^12.6.0",
    "swr": "^2.2.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "tinacms": "^1.5.22",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.7",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "autoprefixer": "^10.4.14",
    "encoding": "^0.1.13",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "nyc": "^15.1.0",
    "postcss": "^8.4.23",
    "postcss-nesting": "^12.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "supabase": "^1.83.7",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "endOfLine": "lf"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm run lint",
      "pnpm run format",
      "git add ."
    ]
  }
}
